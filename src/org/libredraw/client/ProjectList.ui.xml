<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client" xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel>
		<g:MenuBar width="100%" height="34px">
			<g:MenuItem text="Refresh" ui:field="refreshMenu"/>
			<g:MenuItem text="New Project" ui:field="newProjectMenu"/>
			<g:MenuItemSeparator/>
			<g:MenuItem text="Manage Project">
				<g:MenuBar vertical="true">
					<g:MenuItem text="Archive" ui:field="archiveMenu"/>
					<g:MenuItem text="Edit" ui:field="editMenu"/>
					<g:MenuItem text="Delete" ui:field="deleteMenu" enabled="false"/>
					<g:MenuItemSeparator/>
					<g:MenuItem text="Export" ui:field="exportMenu" enabled="false"/>
				</g:MenuBar>
			</g:MenuItem>
		</g:MenuBar>
		<g:ScrollPanel alwaysShowScrollBars="false" width="100%" ui:field="scrollPanel">
			<p1:CellTable ui:field="table" pageSize="10000"/>
		</g:ScrollPanel>
	</g:VerticalPanel>
</ui:UiBinder> 