<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	
	<g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label text="Diagram name:"/>
			<g:Label ui:field="errorLabel" styleName="errorLabel"/>
		</g:HorizontalPanel>
		<g:TextBox height="16px" width="170px" ui:field="nameTextBox"/>
		<g:Label text="Diagram type:"/>
		<g:ListBox ui:field="typeCombo" width="170px" height="16px" name="Diagram Type" enabled="false" selectedIndex="0"/>
		<g:HorizontalPanel styleName="newProjectHorizontal">
			<g:Button ui:field="submitButton">Submit</g:Button>
			<g:AbsolutePanel width="25px" height="20px"/>
			<g:Button ui:field="cancelButton">Cancel</g:Button>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 